\begin_layout Subsection*
Exemplo 01
\end_layout

\begin_layout Standard
Calcule a integral 
\begin_inset Formula $\int_{C}\mathbf{F}\cdot d\mathbf{r}$
\end_inset

, onde 
\begin_inset Formula $\mathbf{F}=z\,\mathbf{i}+xy\,\mathbf{j}-y^{2}\,\mathbf{k}$
\end_inset

 ao longo da curva dada por 
\begin_inset Formula $\mathbf{r}(t)=t^{2}\,\mathbf{i}+t\,\mathbf{j}+\sqrt{t}\,\mathbf{k}$
\end_inset

, 
\begin_inset Formula $0\leq t\leq1$
\end_inset


\end_layout

\begin_layout Subsubsection*
SOL
\end_layout

\begin_layout Standard
Temos
\begin_inset Formula 
\[
\mathbf{F}\left(\mathbf{r}(t)\right)=\sqrt{t}\,\mathbf{i}+t^{3}\,\mathbf{j}-t^{2}\,\mathbf{k}
\]

\end_inset

e
\begin_inset Formula 
\[
\mathbf{v}(t)=2t\,\mathbf{i}+\mathbf{j}+\dfrac{1}{2\sqrt{t}}\,\mathbf{k}
\]

\end_inset

de forma que
\begin_inset Formula 
\begin{align*}
\int_{C}\mathbf{F}\left(\mathbf{r}(t)\right)\cdot d\mathbf{r}= & \int_{0}^{1}\mathbf{F}\left(\mathbf{r}(t)\right)\cdot\mathbf{v}(t)dt\\
= & \int_{0}^{1}\left(2t^{3/2}+t^{3}-\dfrac{1}{2}t^{3/4}\right)dt\\
= & \left[\left(\dfrac{2}{3}\right)\left(\dfrac{2}{5}t^{5/2}\right)+\dfrac{1}{4}t^{4}\right]_{0}^{1}\\
= & \dfrac{17}{20}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection*
Exemplo 02
\end_layout

\begin_layout Standard
Calcule a integral 
\begin_inset Formula $\int_{C}\mathbf{F}\cdot d\mathbf{r}$
\end_inset

, onde 
\begin_inset Formula $\mathbf{F}=z\,\mathbf{i}+y^{2}\,\mathbf{j}-x\,\mathbf{k}$
\end_inset

 ao longo da curva dada por 
\begin_inset Formula $\mathbf{r}(t)=\left(t+1\right)\,\mathbf{i}+e^{t}\,\mathbf{j}+t^{2}\,\mathbf{k}$
\end_inset

, 
\begin_inset Formula $0\leq t\leq2$
\end_inset


\end_layout

\begin_layout Subsubsection*
SOL
\end_layout

\begin_layout Standard
Temos
\begin_inset Formula 
\[
\mathbf{F}\left(\mathbf{r}(t)\right)=t^{2}\,\mathbf{i}+e^{2t}\,\mathbf{j}-\left(t+1\right)\,\mathbf{k}
\]

\end_inset

e
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbf{v}(t)=\mathbf{i}+e^{t}\,\mathbf{j}+2t\,\mathbf{k}
\]

\end_inset

de forma que
\begin_inset Formula 
\begin{align*}
\int_{C}\mathbf{F}\left(\mathbf{r}(t)\right)\cdot d\mathbf{r}= & \int_{0}^{2}\mathbf{F}\left(\mathbf{r}(t)\right)\cdot\mathbf{v}(t)dt\\
= & \int_{0}^{2}\left(e^{3t}+3t^{2}+2t\right)dt\\
= & \left[\dfrac{1}{3}d^{3t}+t^{3}+t^{2}\right]_{0}^{2}\\
= & \left(\dfrac{1}{3}e^{6}+8+4\right)-\dfrac{1}{3}\\
= & \dfrac{1}{3}\left(e^{6}+35\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection*
Exemplo 03
\end_layout

\begin_layout Standard
Seja 
\begin_inset Formula $\mathbf{r}(t)=\sin t\,\mathbf{i}+\cos t\,\mathbf{j}+t\,\mathbf{k}$
\end_inset

 com 
\begin_inset Formula $0\leq t\leq2\pi$
\end_inset

.
 Seja o campo vetorial definido por 
\begin_inset Formula $\mathbf{F}=x\,\mathbf{i}+y\,\mathbf{j}+z\,\mathbf{k}$
\end_inset

.
 Calcule 
\begin_inset Formula $\int_{C}\mathbf{F}\cdot d\mathbf{r}$
\end_inset


\end_layout

\begin_layout Subsubsection*
SOL
\end_layout

\begin_layout Standard
Temos
\begin_inset Formula 
\[
\mathbf{F}\left(\mathbf{r}(t)\right)=\sin t\,\mathbf{i}+\cos t\,\mathbf{j}+t\,\mathbf{k}
\]

\end_inset

e
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbf{v}(t)=-\cos t\,\mathbf{i}+\sin t\,\mathbf{j}+\,\mathbf{k}
\]

\end_inset

de forma que
\begin_inset Formula 
\begin{align*}
\int_{C}\mathbf{F}\left(\mathbf{r}(t)\right)\cdot d\mathbf{r}= & \int_{0}^{2\pi}\mathbf{F}\left(\mathbf{r}(t)\right)\cdot\mathbf{v}(t)dt\\
= & \int_{0}^{2\pi}\left(-\cos t\,\sin t+\cos t\,\sin t+t\right)dt\\
= & \left[\dfrac{1}{2}t^{2}\right]_{0}^{2\pi}\\
= & 2\pi^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection*
Exemplo 04
\end_layout

\begin_layout Standard
Seja 
\begin_inset Formula $\mathbf{r}(t)=\cos^{3}t\,\mathbf{i}+\sin t\,\mathbf{j}+t\,\mathbf{k}$
\end_inset

, onde 
\begin_inset Formula $0\leq t\leq\dfrac{7}{2}\pi$
\end_inset

, como se mostra na figura embaixo.
 Calcule a integral de linha 
\begin_inset Formula $\int_{C}\left(\sin z\, dx+\cos z\, dy-\left(xy\right)^{1/3}dz\right)$
\end_inset


\end_layout

\begin_layout Subsubsection*
SOL
\end_layout

\begin_layout Standard
Para este caso temos
\begin_inset Formula 
\begin{align*}
\dfrac{dx}{dt}= & -3\cos^{2}t\,\sin t\\
\dfrac{dy}{dt}= & 3\cos^{2}t\,\cos t\\
\dfrac{dz}{dt}= & 1
\end{align*}

\end_inset

tal que a integral é
\begin_inset Formula 
\begin{align*}
\int_{C}\left(\sin z\, dx+\cos z\, dy-\left(xy\right)^{1/3}dz\right)= & -\int_{0}^{\frac{7\pi}{2}}3\sin t\,\cos^{2}t\,\sin t\, dt+\int_{0}^{\frac{7\pi}{2}}3\sin t\,\cos^{2}t\,\cos t\, dt-\int_{0}^{\frac{7\pi}{2}}\cos t\,\sin\, t\, dt\\
= & -\int_{0}^{\frac{7\pi}{2}}\cos t\,\sin\, t\, dt\\
= & -\left[\sin^{2}t\right]_{0}^{\frac{7\pi}{2}}\\
= & -\dfrac{1}{2}
\end{align*}

\end_inset


\end_layout

